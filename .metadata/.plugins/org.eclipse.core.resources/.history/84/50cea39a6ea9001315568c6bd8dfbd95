package execution;

import rendering.ConsoleRenderer;
import foundation.Grid;

public class ConsoleExcecuter {
	public static void main(String[] args) {
		int squaresize = 30;
		Grid g = new Grid();
		g.initSquare(squaresize, squaresize);
		
		ConsoleRenderer cr = new ConsoleRenderer();
		cr.render(g, squaresize, squaresize);
		
		g.randomizieSquare(squaresize, squaresize);
		cr.render(g, squaresize, squaresize);
	}
}
