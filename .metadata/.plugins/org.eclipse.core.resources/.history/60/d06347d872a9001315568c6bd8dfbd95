package computation;

public class Utils {
	public static String getNorthNeighborKey(int x, int y) {
		return x+","+(y-1);
	}
	
	public static String getNorthEastNeighborKey(int x, int y) {
		return (x+1)+","+(y-1);
	}
	
	public static String getEastNeighborKey(int x, int y) {
		return (x+1)+","+(y);
	}
}
